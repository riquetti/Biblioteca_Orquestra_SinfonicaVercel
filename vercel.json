{
  "version": 2,
  "builds": [
    {
      "src": "vercel-build.sh",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.8"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "biblioteca/wsgi.py"
    }
  ],
  "frameworks": {
    "django": "4.213"
  }
}